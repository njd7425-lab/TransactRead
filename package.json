{
  "name": "transactread",
  "version": "1.0.0",
  "description": "A lightweight web app for analyzing Ethereum wallet activity",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "seed": "cd server && npm run seed",
    "test": "cd server && npm test",
    "setup": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": ["ethereum", "blockchain", "wallet", "transactions", "ai"],
  "author": "TransactRead Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
